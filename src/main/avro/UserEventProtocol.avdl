@namespace("net.explorviz.avro")
protocol TraceEventProtocol {
	
	enum EventType {
	   CREATED,
	   DELETED,
	   ACCESS_GRANTED,
	   ACCESS_REVOKED,
	   CLONED
	}
	
	record LandscapeToken {
		string value;
		string ownerId;
		string secret;
		long created;
		string alias;
		boolean isRequestedFromVSCodeExtension;
    string projectName;
    string commitId;
	}

  record TokenEvent {
    EventType type;
    LandscapeToken token;
    string clonedToken;
  }
}
